{
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/sandhanam.mp3",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Content-Type": "audio/mpeg",
        "Accept-Ranges": "bytes"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}

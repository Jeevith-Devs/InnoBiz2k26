{
  "rewrites": [
    {
      "source": "/((?!assets|sandhanam\\.mp3|favicon|.*\\.(mp3|svg|png|jpg|jpeg|gif|ico|css|js|json|woff|woff2|ttf|eot)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sandhanam.mp3",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "audio/mpeg"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    }
  ]
}
